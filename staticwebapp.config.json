{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/assets/*",
      "/*.css",
      "/*.js",
      "/*.png",
      "/*.jpg",
      "/*.svg",
      "/*.ico",
      "/*.woff",
      "/*.woff2"
    ]
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html"
    }
  },
  "mimeTypes": {
    ".json": "application/json",
    ".js": "application/javascript",
    ".jsx": "application/javascript",
    ".ts": "text/typescript",
    ".tsx": "text/typescript",
    ".css": "text/css",
    ".woff": "font/woff",
    ".woff2": "font/woff2"
  }
}
